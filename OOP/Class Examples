class Car:
    def __init__(self, make, model, year):
        self.make = make
        self.model = model
        self.year = year

    def display_info(self):
        print(f"Car: {self.year} {self.make} {self.model}")

# Create an object of the Car class
my_car = Car("Toyota", "Corolla", 2020)

# Call the method to display information
my_car.display_info()

class Car:
    def __init__(self, make, model, year):
        self.make = make
        self.model = model
        self.year = year

    def display_info(self):
        print(f"Car: {self.year} {self.make} {self.model}")

    def start_engine(self):
        print(f"{self.make} {self.model}'s engine started.")

# Create an object of the Car class
my_car = Car("Honda", "Civic", 2018)
another_car = Car("Ford", "Focus", 2021)

# Call the methods
my_car.display_info()
my_car.start_engine()

another_car.display_info()
another_car.start_engine()
